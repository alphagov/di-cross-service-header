{
  "name": "di-cross-service-header",
  "version": "0.1.0",
  "description": "A header for services using GOV.UK One Login",
  "main": "index.js",
  "scripts": {
    "build-templates": "node build.js",
    "build-sass": "sass --no-source-map --load-path=node_modules/govuk-frontend src/service-header.scss dist/service-header.css",
    "watch-sass": "sass --no-source-map --load-path=node_modules/govuk-frontend --watch src/service-header.scss dist/service-header.css",
    "build-all": "npm run build-templates && npm run build-sass"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "sass": "^1.60.0"
  },
  "dependencies": {
    "govuk-frontend": "^4.5.0",
    "nunjucks": "^3.2.4"
  }
}
